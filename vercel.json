{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        },
		{
            "src": "population.js",
            "use": "@now/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/",
            "dest": "public/Pages/HomePage/index.html"
        },
		{
			"src":"/Countries/CallingCodes",
			"dest": "public/Pages/CallingCodes/CallingCodes.html"
		},
		{
			"src":"/Countries/Currencies",
			"dest": "public/Pages/Currencies/Currencies.html"
		},
		{
			"src":"/Countries/Demonyms",
			"dest": "public/Pages/Demonyms/Demonyms.html"
		},
		{
			"src":"/Countries/TimeConversions",
			"dest": "public/Pages/TimeConversions/TimeConversions.html"
		},
		{
			"src":"/Countries/LanguagesSpoken",
			"dest": "public/Pages/LanguagesSpoken/LanguagesSpoken.html"
		},
		{
			"src":"/Countries/CountryFlags",
			"dest": "public/Pages/CountryFlags/CountryFlags.html"
		},
		{
			"src":"/Countries/CapitalCities",
			"dest": "public/Pages/CapitalCities/CapitalCities.html"
		},
		{
			"src":"/credits",
			"dest": "public/Pages/Credits/Credits.html"
		}
    ]
}