{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/",
            "dest": "public/Pages/HomePage/index.html"
        },
		{
			"src":"/Countries/CallingCodes",
			"dest": "public/Pages/CallingCodes/CallingCodes.html"
		},
		{
			"src":"/poppulation",
			"dest": "index.js"
		}
    ]
}