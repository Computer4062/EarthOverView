{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node",
            "command": "npm run build"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/",
            "dest": "public/Pages/HomePage/index.html"
        }
    ]
}