{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],

    "buildCommand": "npm run build",
    "installCommand": "npm install"
}